{
  "name": "silverstripe/installer",
  "description": "The SilverStripe Framework Installer",
  "require": {
    "php": ">=5.3.3",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "silverstripe/cms": "3.3.1",
    "silverstripe/framework": "3.3.1",
    "silverstripe/reports": "3.3.1",
    "silverstripe/siteconfig": "3.3.1",
    "silverstripe-themes/simple": "3.1.*",
    "undefinedoffset/sortablegridfield": "dev-master",
    "silverstripe-australia/gridfieldextensions": "^1.2@dev",
    "unclecheese/betterbuttons": "1.2.*"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~3.7"
  },
  "extra": {
    "branch-alias": {
      "3.x-dev": "3.3.x-dev"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
